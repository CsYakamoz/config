{
  "title": "Keyboard shortcuts for Terminal apps",
  "maintainers": ["csyakamoz"],
  "rules": [
    {
      "description": "Toggle Term for (Neo)Vim",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": ["command"]
            }
          },
          "to": [
            {
              "key_code": "j",
              "modifiers": ["left_option"]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": ["^com\\.github\\.wez\\.wezterm$"]
            }
          ]
        }
      ]
    },
    {
      "description": "FNumber when Right Command with Number",
      "manipulators": [
