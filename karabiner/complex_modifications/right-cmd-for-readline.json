{
  "title": "Change right_command+BS,b,d,f to right_option+BS,b,d,f",
  "rules": [
    {
      "description": "Change right_command+BS,b,d,f to right_option+BS,b,d,f",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "delete_or_backspace",
            "modifiers": {
              "mandatory": ["right_command"]
            }
          },
          "to": [
            {
              "key_code": "delete_or_backspace",
              "modifiers": ["right_option"]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "b",
            "modifiers": {
              "mandatory": ["right_command"]
            }
          },
          "to": [
            {
              "key_code": "b",
              "modifiers": ["right_option"]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "d",
            "modifiers": {
              "mandatory": ["right_command"]
            }
          },
          "to": [
            {
              "key_code": "d",
              "modifiers": ["right_option"]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f",
            "modifiers": {
              "mandatory": ["right_command"]
            }
          },
          "to": [
            {
              "key_code": "f",
              "modifiers": ["right_option"]
            }
          ]
        }
      ]
    }
  ]
}
