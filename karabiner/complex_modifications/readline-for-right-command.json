        {
          "type": "basic",
          "from": {
            "key_code": "d",
            "modifiers": {
              "mandatory": ["right_command"]
            }
          },
          "to": [
            {
              "key_code": "d",
              "modifiers": ["left_option"]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f",
            "modifiers": {
              "mandatory": ["right_command"]
            }
          },
          "to": [
            {
              "key_code": "f",
              "modifiers": ["left_option"]
